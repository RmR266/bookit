{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "setup": {
          "commands": ["cd backend"]
        }
      }
    },
    "rootDir": "backend"
  },
  "deploy": {
    "startCommand": "npm run build && npm start"
  }
}
